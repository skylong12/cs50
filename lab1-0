#include <cs50.h>
#include <stdio.h>

int main(void)
{
    float x;
    float y;
    int n = 0;
    do
    {
        x = get_float("startsize:");
    }
    while(x < 9);
    do
    {
        y = get_float("endsize:");
    }
    while(x > y);
    while(x <y)
    {
        x = x*13/12;
        n++;
    }
    printf("years: %i\n", n);

}
